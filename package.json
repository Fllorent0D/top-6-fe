{
  "name": "be-ping",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --configuration production",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "deploy": "npm run build -s && firebase deploy"
  },
  "dependencies": {
    "@angular/animations": "^14.2.3",
    "@angular/cdk": "^14.2.2",
    "@angular/common": "^14.2.3",
    "@angular/compiler": "^14.2.3",
    "@angular/core": "^14.2.3",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^14.2.3",
    "@angular/material": "^14.2.2",
    "@angular/platform-browser": "^14.2.3",
    "@angular/platform-browser-dynamic": "^14.2.3",
    "@angular/router": "^14.2.3",
    "@angular/service-worker": "^14.2.3",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@ngx-translate/core": "^14.0.0",
    "bootstrap": "^5.2.1",
    "core-js": "^2.5.6",
    "firebase": "^9.10.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.5.6",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "^14.2.3",
    "@angular/language-service": "^14.2.3",
    "@angular-devkit/build-angular": "^14.2.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "^5.2.2",
    "@types/jasmine": "^4.3.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.18",
    "codelyzer": "^6.0.2",
    "hads": "^3.0.3",
    "htmlhint": "^1.1.4",
    "https-proxy-agent": "^5.0.1",
    "stylelint": "~14.12.1",
    "stylelint-config-recommended-scss": "~7.0.0",
    "stylelint-config-standard": "~28.0.0",
    "stylelint-scss": "~4.3.0",
    "ts-node": "~10.9.1",
    "tslint": "~5.20.1",
    "typescript": "~4.8.3"
  }
}
